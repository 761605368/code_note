{
	"manifest_version": 2,
	"name": "CSDN复制限制去除",
	"version": "1.0",
	"description": "CSDN复制限制去除",
	"author": "lx",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts":
	[
		{
			"matches": ["https://blog.csdn.net/*"],
			"js": ["js/content-script.js"]
		}
	],

	"permissions":
	[
		"storage"

	]
}